[Unit]
Description=OpenClaw Model Health Check
After=network-online.target
Wants=network-online.target

[Service]
Type=oneshot
WorkingDirectory=%h/.openclaw/workspace
EnvironmentFile=-%h/.env
ExecStart=/usr/bin/env bash %h/.openclaw/workspace/model-health-check.sh
SuccessExitStatus=1
StandardOutput=append:%h/.openclaw/workspace/model-health-check.log
StandardError=append:%h/.openclaw/workspace/model-health-check.log

[Install]
WantedBy=default.target
